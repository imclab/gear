<li><%= link_to_function transfer_to_target.display_name, nil do |page|
	
	if (transfer_to_target.type.to_s == "Location")
		page[:transfer_to_text].replace_html "to location: #{transfer_to_target.display_name}"
		page[:transfer_to_location_id].value=transfer_to_target.id
		page[:transfer_to_person_id].value=""
		page[:transfer_transfer_to_type].value="Location"
	elsif (transfer_to_target.type.to_s == "Person")
		page[:transfer_to_text].replace_html "to person: #{transfer_to_target.display_name}"
		page[:transfer_to_person_id].value=transfer_to_target.id
		page[:transfer_to_location_id].value=""
		page[:transfer_transfer_to_type].value="Person"
	else
		page[:transfer_to_text].replace_html transfer_to_target.type
	end
 end
%></li>

